{
  "name": "acpx-orchestrator",
  "version": "4.0.6",
  "description": "Enhanced terminal AI agent orchestrator with parallel execution, health checks, and workflow presets",
  "author": "qriiz112",
  "email": "christianovianto@gmail.com",
  "homepage": "https://github.com/qriiz112/acpx-orchestrator",
  "repository": "https://github.com/qriiz112/acpx-orchestrator.git",
  "license": "MIT",
  "tags": ["agents", "orchestration", "cli", "automation", "acp", "ai", "terminal"],
  "binaries": {
    "required": ["acpx"],
    "optional": ["opencode", "pi", "kimi", "kilo", "codex", "claude", "gemini"]
  },
  "install": {
    "command": "npm install -g acpx"
  },
  "requires": {
    "bins": ["acpx"]
  },
  "suggests": {
    "bins": ["opencode", "pi", "kimi", "kilo", "codex", "claude", "gemini"]
  },
  "warnings": {
    "scope": "Agents write files to current directory and spawn subagents",
    "install": "npm install -g acpx (verify package before install)",
    "note": "No credentials requested, always=false"
  },
  "always": false,
  "autonomous": true
}
